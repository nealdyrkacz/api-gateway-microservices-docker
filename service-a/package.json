{
  "name": "service-a",
  "version": "1.0.0",
  "description": "A simple service",
  "engines": {
    "node": "12.19.0"
  },
  "scripts": {
    "start": "node ./build/server.js",
    "build": "tsc",
    "tsc": "rm -r -f ./build && tsc",
    "db:migrate": "npx sequelize db:migrate",
    "servicea:dev:start": "(ts-node-dev --poll --tree-kill --respawn --transpile-only ./src/server.ts)",
    "servicea:dev:stop": "pkill -f 'service-a'"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "0.0.44",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.7",
    "nodemon": "^2.0.2",
    "ts-node-dev": "^1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "os": "^0.1.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.0.5",
    "uuid": "^8.2.0"
  }
}
